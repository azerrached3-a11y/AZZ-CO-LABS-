{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/mission",
      "dest": "/mission.html"
    },
    {
      "src": "/jobboat",
      "dest": "/jobboat.html"
    },
    {
      "src": "/invest",
      "dest": "/invest.html"
    },
    {
      "src": "/outwings",
      "dest": "/outwings.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
