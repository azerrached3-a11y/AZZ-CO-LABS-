{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/mission",
      "dest": "/mission.html"
    },
    {
      "src": "/jobboat",
      "dest": "/jobboat.html"
    },
    {
      "src": "/invest",
      "dest": "/invest.html"
    },
    {
      "src": "/outwings",
      "dest": "/outwings.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
